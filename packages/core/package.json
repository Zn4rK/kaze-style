{
  "name": "@kaze-style/core",
  "version": "0.9.0",
  "license": "MIT",
  "author": "Taishi Naritomi",
  "description": "Kaze [é¢¨] zero-runtime CSS in JS",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/taishinaritomi/kaze-style.git",
    "directory": "packages/core"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
